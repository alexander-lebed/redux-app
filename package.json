{
  "name": "wtalk-messenger",
  "version": "1.0.0",
  "description": "Web Messanger (MERN and Redux)",
  "main": "index.js",
  "author": "alexander-lebed",
  "license": "UNLICENSED",
  "engines": {
    "node": "11.x"
  },
  "scripts": {
    "build": "webpack --progress --colors --watch",
    "postinstall": "webpack --config webpack.config.prod.js --progress --optimize-minimize",
    "start": "node server.js",
    "dev": "concurrently \"webpack --progress --colors --watch\" \"nf start -j Procfile.dev\"",
    "start-ui": "webpack-dev-server --open",
    "start-api": "node server.js",
    "test": "jest --coverage",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix --cache",
    "flow": "flow"
  },
  "jest": {
    "collectCoverage": true,
    "testURL": "http://localhost/",
    "setupTestFrameworkScriptFile": "<rootDir>/src/helpers/configureEnzyme.js"
  },
  "dependencies": {

  },
  "devDependencies": {
    
  }
}
